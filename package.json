{
    "devDependencies": {
        "atma": ">0.9.26",
        "atma-loader-babel": ">0.0.0",
        "atma-loader-less": ">0.0.0"
    },
    "scripts": {
        "test": "atma test"
    },
    "atma": {
        "plugins": [
            "atma-loader-babel",
            "atma-loader-less"
        ],
        "settings": {
            "atma-loader-babel": {
                "extensions": [
                    "es6",
                    "test"
                ]
            }
        }
    }
}